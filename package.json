{
  "name": "@kitiumai/test-db",
  "version": "2.0.0",
  "description": "Enterprise-ready test database utilities for PostgreSQL and MongoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./postgres": {
      "types": "./dist/postgres/index.d.ts",
      "import": "./dist/postgres/index.js",
      "default": "./dist/postgres/index.js"
    },
    "./mongodb": {
      "types": "./dist/mongodb/index.d.ts",
      "import": "./dist/mongodb/index.js",
      "default": "./dist/mongodb/index.js"
    },
    "./utils": {
      "types": "./dist/utils/config.d.ts",
      "import": "./dist/utils/config.js",
      "default": "./dist/utils/config.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kitium-ai/test-db.git"
  },
  "homepage": "https://github.com/kitium-ai/test-db#readme",
  "bugs": {
    "url": "https://github.com/kitium-ai/test-db/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "changeset": "ensure-changeset && changeset",
    "version": "changeset version",
    "publish:set-token": "set-npm-token",
    "publish:check": "npm whoami --registry https://registry.npmjs.org || npm login --registry https://registry.npmjs.org",
    "publish:login": "npm login --registry https://registry.npmjs.org",
    "publish:package": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:otp": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org --otp",
    "publish:dry-run": "pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "publish:dry-run:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "release": "pnpm version && pnpm install --lockfile-only"
  },
  "keywords": [
    "database",
    "testing",
    "postgresql",
    "mongodb",
    "test-utilities",
    "enterprise",
    "kitiumai",
    "kitium",
    "test-database",
    "database-testing",
    "postgres",
    "postgres-testing",
    "mongodb-testing",
    "test-helpers",
    "test-setup",
    "integration-testing",
    "database-fixtures",
    "test-data",
    "sql",
    "nosql",
    "pg",
    "mongo",
    "test-db",
    "database-utilities",
    "db-testing",
    "test-framework",
    "jest",
    "vitest",
    "mocha",
    "test-runner",
    "ci-cd",
    "continuous-integration",
    "devops",
    "quality-assurance",
    "qa",
    "typescript",
    "javascript",
    "nodejs",
    "node",
    "test-automation",
    "database-migration",
    "schema-testing",
    "data-seeding",
    "test-isolation",
    "transaction-testing",
    "database-cleanup",
    "temporary-database",
    "test-lifecycle"
  ],
  "author": "Kitium AI",
  "license": "MIT",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "!dist/**/__tests__/**",
    "!dist/**/examples/**",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "dependencies": {
    "@kitiumai/logger": "^2.0.0",
    "dotenv": "^16.3.1",
    "mongodb": "^6.3.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@kitiumai/config": "^2.0.0",
    "@kitiumai/lint": "^2.0.0",
    "@kitiumai/scripts": "^1.0.0",
    "@kitiumai/test-core": "^2.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "graphql": "^16.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module"
}
